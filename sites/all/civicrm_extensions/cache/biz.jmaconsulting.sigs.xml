<?xml version="1.0"?>
<extension key="biz.jmaconsulting.sigs" type="module">
  <downloadUrl>https://github.com/JMAConsulting/biz.jmaconsulting.sigs/archive/v1.1.zip</downloadUrl>
  <file>sigs</file>
  <name>Email Signatures</name>
  <description>This extension allows you to upload an image that can be used as a token while sending emails in order to display your signature.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>JMA Consulting</author>
    <email>joe.murray@jmaconsulting.biz</email>
  </maintainer>
  <releaseDate>2014-09-05</releaseDate>
  <version>1.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>4.3</ver>
    <ver>4.4</ver>
    <ver>4.5</ver>
  </compatibility>
  <urls>
    <url desc="Support">https://github.com/JMAConsulting/biz.jmaconsulting.sigs/issues?state=open</url>
    <url desc="Documentation">https://github.com/JMAConsulting/biz.jmaconsulting.sigs/blob/1.0/README.md</url>
  </urls>
  <comments/>
  <civix>
    <namespace>CRM/Sigs</namespace>
  </civix>
</extension>
